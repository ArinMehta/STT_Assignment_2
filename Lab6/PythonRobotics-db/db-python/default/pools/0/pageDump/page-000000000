    aycycxbxbyob_xob_yvstackobstaclecoorobs_arrayboundbound_obsfind_neighborobob_setneighbor_setcoordtop_neibottom_neileft_neiright_neilt_neirt_neilb_neirb_neifind_node_indexnode_listindtarget_nodefind_pathopen_listclosed_listflagopen_coordinate_listclosed_coordinate_listtempelementnew_gele_nodenode_to_coordinatecoordinate_listcheck_node_coincideclose_ls1closed_ls2
